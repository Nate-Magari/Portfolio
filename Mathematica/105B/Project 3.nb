   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "We", " ", "know", " ", "that", " ", "we", " ", "can", " ", "replace", 
      " ", "the", " ", 
      RowBox[{"d\[Theta]", "/", "dt"}], " ", "with", " ", 
      RowBox[{"l", "/", 
