   RowBox[{
    RowBox[{"expr", " ", "=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"Sec", "[", "\[Theta]", "]"}], 
       RowBox[{"Tan", "[", "\[Theta]", "]"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Sec", "[", "\[Theta]", "]"}], " ", "+", " ", 
         RowBox[{"Tan", "[", "\[Theta]", "]"}]}], ")"}]}], " ", "+", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Sec", "[", "\[Theta]", "]"}], " ", "-", " ", 
        RowBox[{"Tan", "[", "\[Theta]", "]"}]}], ")"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{
     RowBox[{
     "\"\<The RHS of this equation is obtained from the LHS by tacking on a \
//FullSimplify:\\n\\n\>\"", "expr"}], ",", "\"\< = \>\"", ",", " ", 
     RowBox[{"expr", "//", "FullSimplify"}]}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Let", "'"}], "s", " ", "check", " ", "its", " ", "work"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{
     RowBox[{"Style", "[", 
      RowBox[{"\"\<\\n** Quick check **\>\"", ",", "Blue"}], "]"}], ",", 
     "\"\<\\nFrom \!\(\*SuperscriptBox[\(Sin\), \(2\)]\)[\[Theta]] + \
\!\(\*SuperscriptBox[\(Cos\), \(2\)]\)[\[Theta]] = 1:   \!\(\*SuperscriptBox[\
\(Sec\), \(2\)]\)[\[Theta]] = \!\(\*SuperscriptBox[\(Tan\), \
\(2\)]\)[\[Theta]] + 1   and   \!\(\*SuperscriptBox[\(Tan\), \
\(2\)]\)[\[Theta]] = \!\(\*SuperscriptBox[\(Sec\), \(2\)]\)[\[Theta]] - \
1\>\""}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{
    "\"\<So \>\"", ",", "expr", ",", 
     "\"\<\\n\\n= Sec[\[Theta]] - Tan[\[Theta]] + Tan[\[Theta]] \
\!\(\*SuperscriptBox[\(Sec\), \(2\)]\)[\[Theta]] + Sec[\[Theta]] \
\!\(\*SuperscriptBox[\(Tan\), \(2\)]\)[\[Theta]]\\n= Sec[\[Theta]] - Tan[\
\[Theta]] + Tan[\[Theta]](\!\(\*SuperscriptBox[\(Tan\), \(2\)]\)[\[Theta]] + \
1) + Sec[\[Theta]](\!\(\*SuperscriptBox[\(Sec\), \(2\)]\)[\[Theta]] - 1) = \>\
\"", ",", 
     RowBox[{"expr", "//", "FullSimplify"}], ",", "\"\<  \>\"", ",", 
