Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", "5", " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Remove", "[", "\"\<Global`*\>\"", "]"}], "\[IndentingNewLine]", 
